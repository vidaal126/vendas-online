generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String    @id @default(uuid())
  name     String
  email    String @unique
  phone    String
  cpf      String @unique
  password String
}
